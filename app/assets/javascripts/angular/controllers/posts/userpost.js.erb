app.controller('UserPostCtrl',['$scope', '$http', '$location', '$routeParams', function($scope, $http, $location, $routeParams) {

  var url = "<%= ENV['URL'] %>" || "http://localhost:3000/";
  
  console.log($routeParams.user_id);

  $http.get(url + $routeParams.user_id + 'posts').success(function(response){
    $scope.post = response;
  });

}]);
