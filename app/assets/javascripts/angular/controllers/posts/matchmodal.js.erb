app.controller('ModalInstanceCtrl', function ($scope, $modalInstance, items, $location) {
  console.log("itworks");
  $scope.items = items;
  $scope.selected = {
    item: $scope.items[0]
  };

  $scope.ok = function () {
    $modalInstance.close($scope.selected.item);
    $location.path('/mymatches');
  };

  $scope.cancel = function () {
    $modalInstance.dismiss('cancel');
  };

});